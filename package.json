{
  "name": "metronome",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sc": "shadow-cljs",
    "server": "shadow-cljs stop && shadow-cljs start",
    "test:watch": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "lint": "clj-kondo --lint src",
    "format": "zprint-clj --hang -i \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./",
    "clean": "rimraf public/js",
    "shadow:watch": "cross-env NODE_ENV=develop shadow-cljs watch app",
    "shadow:release": "shadow-cljs release app",
    "postcss:build": "cross-env TAILWIND_MODE=build postcss ./src/app/ui/style.css -o ./public/css/style.css --verbose",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss ./src/app/ui/style.css -o ./public/css/style.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss ./src/app/ui/style.css -o ./public/css/style.css --verbose",
    "start": "run-p -l *:watch",
    "build": "run-s clean *:release",
    "tauri": "tauri"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.4",
    "@testing-library/dom": "^7.19.0",
    "@testing-library/react": "^10.4.3",
    "highlight.js": "10.1.1",
    "marked": "^1.1.0",
    "taiko-diagnostics": "~0.4.0",
    "clj-kondo": "2021.12.1",
    "shadow-cljs": "2.17.8",
    "cross-env": "^7.0.3",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "rimraf": "~3.0.0",
    "serve-handler": "~6.1.3",
    "taiko": "^1.0.25",
    "tailwindcss": "^3.0.0",
    "zprint-clj": "~0.8.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "create-react-class": "~15.7.0",
    "postcss": "^8.4.4",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  }
}